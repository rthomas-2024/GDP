<robot name="Gantry_CAD">

	<link name="world">
	</link>

	<joint name="base_joint" type="fixed">
		<!-- because of how the STL has been loaded in, have to rotate by 90 deg and translate slightly-->
		<origin xyz="0 0 0" rpy="1.570796327 0 1.570796327"/>
		<parent link="world"/>
		<child link="base_link"/>
	</joint>

	<!-- links the world to the corner (origin) of the gantry frame-->
    <link name="base_link">
        <visual>
	        <origin xyz="-0.01282036 -0.00465 -0.00316703" rpy="0 0 0"/>
            <geometry>
		        <mesh filename="GANTRY_FRAME.STL" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="grey">
                <color rgba="0.5 0.5 0.5 1"/> 
            </material>
        </visual>
        <collision>
            <geometry>
                <box size="1 1 1"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="1.0"/> <!-- Mass in kilograms -->
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
    </link>

	<joint name="yaxis_joint" type="prismatic">
		
		<origin xyz="0.28605 0.515 0.84441" rpy="0 0 0"/>
		<parent link="base_link"/>
		<child link="yaxis_link"/>

		<axis xyz="0 0 1"/>
		<limit lower="0" upper="1.1" velocity="100" effort="100"/>		
	</joint>

	<link name="yaxis_link">
		<visual>
			<origin xyz="-0.00995999 -0.05 -0.52094043" rpy="0 0 0"/>
			<geometry>
				<mesh filename="YAXIS.STL" scale="0.001 0.001 0.001"/>
			</geometry>
			<material name="red">
				<color rgba="1 0 0 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="1 1 1"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1.0"/>
			<!-- Mass in kilograms -->
			<inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
		</inertial>
	</link>

	<joint name="zaxis_joint" type="prismatic">

		<origin xyz="0.40184 0.5385 0.042" rpy="0 0 0"/>
		<parent link="yaxis_link"/>
		<child link="zaxis_link"/>

		<axis xyz="1 0 0"/>
		<limit lower="-0.34" upper="0.34" velocity="100" effort="100"/>
	</joint>

	<link name="zaxis_link">
		<visual>
			<origin xyz="-0.00998167 -1.00462 -0.56970911" rpy="0 0 0"/>
			<geometry>
				<mesh filename="ZAXIS.STL" scale="0.001 0.001 0.001"/>
			</geometry>
			<material name="green">
				<color rgba="0 1 0 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="1 1 1"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1.0"/>
			<!-- Mass in kilograms -->
			<inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
		</inertial>
	</link>

	<joint name="gimbal_interface_joint" type="prismatic">

		<origin xyz="0 -0.36733 0.117" rpy="0 0 0"/>
		<parent link="zaxis_link"/>
		<child link="gimbal_interface_link"/>

		<axis xyz="0 1 0"/>
		<limit lower="-0.39" upper="0.4" velocity="100" effort="100"/>
	</joint>

	<link name="gimbal_interface_link">
		<visual>
			<origin xyz="-0.00594166 -0.16590892 -0.68670911" rpy="0 0 0"/>
			<geometry>
				<mesh filename="GIMBAL_INTERFACE.STL" scale="0.001 0.001 0.001"/>
			</geometry>
			<material name="blue">
				<color rgba="0 0 1 1"/>
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="1 1 1"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1.0"/>
			<!-- Mass in kilograms -->
			<inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
		</inertial>
	</link>

	<joint name="yaw_axis_joint" type="fixed">
		<origin xyz="0.002 -0.055 0" rpy="0 1.570796327 0"/>
		<parent link="gimbal_interface_link"/>
		<child link="yaw_axis_link"/>

	</joint>

	<link name="yaw_axis_link">
		<visual>
			<origin xyz="-0.76425525 -0.8042663 -1.21712" rpy="0 0 0"/>
			<geometry>
				<mesh filename="YAWAXIS.STL" scale="0.001 0.001 0.001"/>
			</geometry>
			<material name="grey">
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="1 1 1"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1.0"/>
			<!-- Mass in kilograms -->
			<inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
		</inertial>
	</link>

	<joint name="pitch_axis_joint" type="revolute">
		<!-- left/right , up/down, forwards/backwards-->
		<origin xyz="-0.177 0.0155 -0.0125" rpy="0 0 0"/>
		<parent link="yaw_axis_link"/>
		<child link="pitch_axis_link"/>

		<axis xyz="0 1 0"/>
		<limit effort="100" velocity="2" lower="-1.570796327" upper="1.570796327"/>
	</joint>

	<link name="pitch_axis_link">
		<visual>
			<origin xyz="-0.58725525 -0.8197663 -1.20462" rpy="0 0 0"/>
			<geometry>
				<mesh filename="PITCHAXIS.STL" scale="0.001 0.001 0.001"/>
			</geometry>
			<material name="red">
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="1 1 1"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1.0"/>
			<!-- Mass in kilograms -->
			<inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
		</inertial>
	</link>

	<joint name="roll_axis_joint" type="fixed">
		<!-- forwards/backwards, up/down, left/right-->
		<origin xyz="-0.59 -0.82 -1.2" rpy="0 0 0"/>
		<parent link="pitch_axis_link"/>
		<child link="roll_axis_link"/>

	</joint>

	<link name="roll_axis_link">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="ROLLAXIS.STL" scale="0.001 0.001 0.001"/>
			</geometry>
			<material name="green">
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="1 1 1"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1.0"/>
			<!-- Mass in kilograms -->
			<inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
		</inertial>
	</link>

	<joint name="end_eff_interface_joint" type="fixed">
		<!-- forwards/backwards, up/down, left/right-->
		<origin xyz="0.57 1 1.253" rpy="0 0 -1.570796327"/>
		<parent link="roll_axis_link"/>
		<child link="end_eff_interface_link"/>

	</joint>

	<link name="end_eff_interface_link">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="END_EFF_INTERFACE.STL" scale="0.001 0.001 0.001"/>
			</geometry>
			<material name="blue">
			</material>
		</visual>
		<collision>
			<geometry>
				<box size="1 1 1"/>
			</geometry>
		</collision>
		<inertial>
			<mass value="1.0"/>
			<!-- Mass in kilograms -->
			<inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
		</inertial>
	</link>
</robot>