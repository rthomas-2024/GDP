<robot name="target">
  <link name="base_link">
    <visual>
      <geometry>
        <box size="3 2 2"/> <!-- Width, Height, Depth -->
      </geometry>
      <material name="blue">
        <color rgba="0 1 0 1"/> <!-- Red, Green, Blue, Alpha -->
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="3 2 2"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/> <!-- Mass in kilograms -->
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
  </link>
</robot>